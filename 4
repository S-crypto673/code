// 08_fibonacci_n_terms.c
#include <stdio.h>

int main(void) {
    int n;
    printf("Enter number of terms: ");
    scanf("%d", &n);

    int a = 0, b = 1;
    printf("Fibonacci: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", a);
        int next = a + b;
        a = b;
        b = next;
    }
    printf("\n");
    return 0;
}
